  },
  "homepage": "https://github.com/heysdc/react-webpack-demo#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
